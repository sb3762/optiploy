<!DOCTYPE hibernate-configuration PUBLIC
	"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">

<hibernate-configuration>
    <session-factory>
    	<mapping class="com.optiploy.model.Agent"/>    	
    	<mapping class="com.optiploy.model.Application"/> 
    	<mapping class="com.optiploy.model.Attribute"/>
    	<mapping class="com.optiploy.model.AttributeType"/>
    	<mapping class="com.optiploy.model.Environment"/>
    	<mapping class="com.optiploy.model.Function"/> 
    	<mapping class="com.optiploy.model.Instance"/>                
