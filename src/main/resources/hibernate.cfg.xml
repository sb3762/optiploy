        <mapping class="com.optiploy.model.Log"/>
        <mapping class="com.optiploy.model.LogFile"/>         
        <mapping class="com.optiploy.model.Module"/>
        <mapping class="com.optiploy.model.Progress"/>          
        <mapping class="com.optiploy.model.Property"/>
        <mapping class="com.optiploy.model.Release"/>
        <mapping class="com.optiploy.model.Report"/>
        <mapping class="com.optiploy.model.ReportMenu"/>
        <mapping class="com.optiploy.model.Role"/>
        <mapping class="com.optiploy.model.Rule"/> 
        <mapping class="com.optiploy.model.RuleType"/> 
        <mapping class="com.optiploy.model.Script"/>
        <mapping class="com.optiploy.model.User"/>
